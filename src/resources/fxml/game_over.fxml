<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Text?>

<StackPane xmlns="http://javafx.com/javafx/17.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <VBox spacing="20" alignment="CENTER" style="-fx-background-color: #282828; -fx-padding: 20; -fx-background-radius: 15;">
        <!-- 游戏失败的文本 -->
        <Text fx:id="gameOverText" text="You Failed" style="-fx-font-size: 50; -fx-fill: red; -fx-font-weight: bold; -fx-effect: dropshadow(gaussian, rgba(0, 0, 0, 0.5), 10, 0.5, 0, 0);" />

    </VBox>
</StackPane>
